@model IEnumerable<ePaperLive.DBModel.Subscriber_Tranx>

@{
    ViewBag.Title = "Transactions";
}

    <div id="main">

        <!-- ======= Breadcrumbs ======= -->
        <section id="breadcrumbs" class="breadcrumbs">
            <div class="container">

                <ol>
                    <li><a href="@Url.Action("index", "home")">Home</a></li>
                    <li>@ViewData["Title"]</li>
                </ol>
                <h2>@ViewData["Title"]</h2>

            </div>
        </section><!-- End Breadcrumbs -->
        <!-- ======= Wizard Section ======= -->
        <section id="portfolio-details" class="portfolio-details">
            <div class="container">

                <div class="row gy-4">
                    <div class="col-lg-12">
                        <!-- start:wizrd -->
                        <table class="table table-hover table-responsive table-striped small" id="tranxList">
                            <thead>

                                <tr>

                                    <th>
                                        @Html.DisplayNameFor(model => model.EmailAddress)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CardOwner)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CardType)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CardExp)
                                    </th>

                                    <th>
                                        @Html.DisplayNameFor(model => model.TranxAmount)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.TranxDate)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.RateID)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.TranxType)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.OrderID)
                                    </th>
                          
                                    <th>Options</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>

                                        <td>
                                            @Html.DisplayFor(modelItem => item.EmailAddress)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CardOwner)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CardType)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CardExp)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.TranxAmount)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.TranxDate)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.RateID)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.TranxType)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.OrderID)
                                        </td>

                               
                                        <td>
                                            @Html.ActionLink("Edit", "Edit", new { id = item.Subscriber_TranxID }) |
                                            @Html.ActionLink("View", "Details", new { id = item.Subscriber_TranxID }) 
                                            @*@Html.ActionLink("Delete", "Delete", new { id = item.Subscriber_TranxID })*@
                                        </td>
                                    </tr>
                                }
                            </tbody>

                        </table>
                        <!-- end:wizrd -->
                    </div>
                </div>

            </div>
        </section><!-- End Wizard Section -->

    </div>




@section scripts{
    <script>
        $(document).ready(function () {
            $('#tranxList').DataTable({
               "pageLength": 25
            });
        });


    </script>
}