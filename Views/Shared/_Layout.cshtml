                        }
                    }
                </ul>

                <i class="bi bi-list mobile-nav-toggle"></i>
            </nav><!-- .navbar -->
            @{
                try
                {
                    if (!Request.IsAuthenticated)
                    {
                        <div class="d-flex m-3 text-center">
                            <a href="#" class="btn-sign-in" data-bs-toggle="modal" data-bs-target="#login-modal">Sign In</a>
                        </div>
                    }

                }
                catch (Exception ex)
                {

                    throw ex;
                }
            }


        </div>
    </header><!-- End Header -->


    @RenderBody()

