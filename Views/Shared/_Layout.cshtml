                                <li class="dropdown">
                                    <a href="#"><span>My Account</span> <i class="bi bi-chevron-down"></i></a>
                                    <ul>
                                        @*<li>@Html.ActionLink(User.Identity.Name, "Dashboard", new { controller = "Account" }, new { area = "" })</li>*@
                                        <li>@Html.ActionLink("Change Password", "changepassword", "manage")</li>
                                        <li>@Html.ActionLink("Update Profile", "userprofile", "account")</li>
                                        <li><a class="nav-link scrollto" href="#testimonials">Request Refund</a></li>
                                        @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                                        {
                                            @Html.AntiForgeryToken()
