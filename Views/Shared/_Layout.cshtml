                <a href="@Url.Action("index", "home")"><img src="@Url.Content("~/assets/img/epaper-jol-logo.png")" alt="" class="img-fluid"></a>
            </div>

            <nav id="navbar" class="navbar order-last order-lg-0">
                <ul>
                    @{
